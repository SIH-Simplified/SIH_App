    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        @import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@600&family=Poppins:wght@300&display=swap');

        body {
            background: #f5f5f5;
        }

        .navigation-bt {
            font-size: min(1.2vw, 1.5rem);
            color: black;
            font-family: 'Poppins', sans-serif;
            border: none;
            border-radius: 7px;
            float: left;
            margin-left: 20%;
            ;
        }

        #burger {
            color: black;
            position: fixed;
            top: 12%;
            font-size: min(7vw, 3rem);
            left: 5%;
            visibility: hidden;
            z-index: 16;
        }

        #navbar {
            width: 13%;
            height: 100%;
            position: fixed;
            background: white;
            border-right: 1px solid #f5f5f5;
            z-index: 3;
        }

        #maindiv {
            position: absolute;
            left: 13%;
            width: 87%;
            height: 93%;
            top: 0%;
        }

        #container1 {
            width: 98%;
            position: relative;
            left: 1%;
            background: white;
            border-radius: 5px;
            padding: 30px;
            font-family: 'Poppins', sans-serif;
            margin-top: 1.5%;
            display: flex;
        }

        #logo {
            position: relative;
            left: 15%;
        }

        #title {
            font-size: min(3vw, 3rem);
            font-weight: bold;
        }

        .row1_child {
            background: white;
            border-radius: 7px;
            box-shadow: 2px 2px 3px 3px lightgrey;
            width: 100%;

        }

        #row1 {
            width: 98%;
            margin-left: 1%;
        }

        #row2 {
            width: 98%;
            margin-left: 1%;
            height: 55%;
        }

        #row3 {
            width: 98%;
            margin-left: 1%;
            height: 40%;
        }

        .row1_child_font1 {
            font-family: 'Poppins';
            font-style: normal;
            font-weight: bold;
            font-size: min(3vw, 2.5rem);
            margin: 10%;


            color: #012A60;

        }

        .row1_child_font2 {
            font-family: 'Poppins';
            font-style: normal;
            font-weight: bold;
            font-size: min(3vw, 5rem);
            margin: 10%;
            position: relative;
            left: 55%;
            color: #012A60;

        }
    </style>
</head>

<body>
    <span class="glyphicon glyphicon-menu-hamburger" id="burger"></span>
    <div id="navbar">
        <center><br><br>

            <font class="navigation-bt">
                <button type="button" class="btn btn-primary">Exams</button>
            </font><br><br>
            <font class="navigation-bt">
                <button type="button" class="btn btn-primary">View Portfolio</button>
            </font><br>
            <font class="navigation-bt">
                <a class="btn btn-primary" href="http://localhost:3000/superAdmin/scheduleMettings"
                    role="button">Schedule Mettings</a>
            </font><br>
            <font class="navigation-bt">
                <button type="button" class="btn btn-primary">Exam list</button>
            </font><br>
            <font class="navigation-bt">
                <a class="btn btn-primary" href="http://localhost:3000/superAdmin/assignAdmin" role="button">
                    Allot Admin rolls
                </a>
            </font><br>

        </center>
    </div>
    <div id="maindiv">
        <div id="container1">
            <span class="row" style="width:100%;">
