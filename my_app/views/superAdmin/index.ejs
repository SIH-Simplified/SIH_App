                    <img src="/images/govt.svg">
                </span>

                <span class="col-md-8">
                    <center>
                        <font id="title"><span style="color:orangered">MAHARASHTRA GOVERNMENT</span><br>
                            Online recruitment management system</font>
                    </center>
                </span>
                <span class="col-md-2">
                    <img src="/images/logo.svg" id="logo">
                </span>
            </span>
        </div>
        <br>
        <span class="row" id="row1">
            <span class="col-md-3">
                <div class="row1_child">
                    <br>
                    <font class="row1_child_font1"> Total affiliated<br></font>
                    <font class="row1_child_font1"> schools</font>
                    <br><br>
                    <font class="row1_child_font2"> 295</font>
                </div>
            </span>
            <span class="col-md-3">
                <div class="row1_child">
                    <br>
                    <font class="row1_child_font1"> Total recruited
                        <br>
                    </font>
                    <font class="row1_child_font1"> teachers</font>
                    <br><br>
                    <font class="row1_child_font2"> 295</font>
                </div>
            </span>
            <span class="col-md-3">
                <div class="row1_child">
                    <br>
                    <font class="row1_child_font1"> Total examinations
                        <br>
                    </font>
                    <font class="row1_child_font1"> offerred</font>
                    <br><br>
                    <font class="row1_child_font2"> 295</font>
                </div>
            </span>
            <span class="col-md-3">
                <div class="row1_child">
                    <br>
                    <font class="row1_child_font1"> Total applicants
                        <br>
                    </font>
                    <font class="row1_child_font1"> 2022 </font>
                    <br><br>
                    <font class="row1_child_font2"> 295</font>
                </div>
            </span>
        </span>

        <br>
        <span class="row" id="row2">
            <span class="col-md-3">
                <div class="row1_child" style="overflow:auto;height:48.5rem;">
                    <br>
                    <font class="row1_child_font1">Manage Applications</font>
                    <% if(typeof application !=="undefined" && application.length !==0) {%>
                        <% for(let i=0;i<application.length;i++) {%>
                            <div style="height:100%;" id="manageapplication">
                                <h2>
                                    <%= application[i].teacher_name %>
                                </h2>
                                <h3 class="h5">
                                    <%= application[i].subject %>
                                </h3>
                                <p>
                                    <%= application[i].message %>
                                </p>
                            </div>
                            <% } %>
                                <% } %>
                </div>
            </span>


            <span class="col-md-9">
                <span class="row" style="height:46%;">
                    <span class="col-md-4">
                        <div class="row1_child" style="height:25%;">
                            <br>
                            <center>
                                <font class="row1_child_font1"> Allot admin roles</font><br><br>
                                <font class="row1_child_font1"
                                    style="border:1px solid grey;border-radius:7px;padding:10px;"> 295 roles alloted
                                </font>
                                <!-- <font class="row1_child_font1"
                                    style="border:1px solid grey;border-radius:7px;padding:10px;float:left;background:#6853F8;color:white;"> -->
                                <form action="http://localhost:3000/superAdmin/admins" method=" get">
                                    <button type="button" class="btn btn-primary"> View school summary</button>
                                </form>
                                <!-- </font> -->
                            </center>
                        </div>
                        <br>
                        <div class="row1_child" style="height:25%;">
                            <br>
                            <center>
                                <font class="row1_child_font1"> push updates </font><br><br>
                                <!-- <font class="row1_child_font1"
                                    style="border:1px solid grey;border-radius:7px;padding:10px;"> view updates</font> -->
                                <form action="http://localhost:3000/superAdmin/pushUpdates/create">
                                    <button type="submit" class="btn btn-info"
                                        style="border:1px solid grey;border-radius:7px;padding:10px;">
                                        Push Updates</button>
                                </form>

                                <br><br><br><br>
                                <font class="row1_child_font1"
                                    style="border:1px solid grey;border-radius:7px;padding:10px;background:#6853F8;color:white;">
                                    add an update</font>
                            </center>
                        </div>
                    </span>

                    <span class="col-md-8">
                        <div class="row1_child" style="height:25%;overflow:auto;">
                            <br>
                            <font class="row1_child_font1"> Schedule Trainings </font>
                            <font style="color:#6853F8;font-size:min(3vw,3rem); ">
                                <a class="btn btn-primary" href="http://localhost:3000/superAdmin/scheduleMettings"
                                    role="button">+</a>
                            </font><br>
                            <% if(typeof training !=="undefined" && training.length !==0) {%>
                                <% for(let i=0;i<training.length;i++) {%>
                                    <div style="height:100%;" id="schedule">
                                        <h2>
                                            <%= training[i].title %>
                                        </h2>
                                        <p class="display-2">
                                            <%= training[i].dataFrom %>
                                        </p>
                                        <p class="display-2">
                                            <%= training[i].dateTo %>
                                        </p>
                                        <p class="display-2">
                                            <%= training[i].location %>
                                        </p>
                                        <a class="btn btn-primary" href="<%= training[i].training_pdf %>" role="button">
                                            Training Doc
                                        </a>
                                    </div>
                                    <% } %>
                                        <% } %>
                        </div>
                        <br>
                        <div class="row1_child" style="height:25%;">
                            <br>
                            <center>
                                <font class="row1_child_font1">Manage teacher’s transfer requests </font><br><br>
                                <font class="row1_child_font1"
                                    style="border:1px solid grey;border-radius:7px;padding:10px;"> 27 pending </font>
                                <a class="btn btn-danger" href="http://localhost:3000/superAdmin/teacherTransfer"
                                    role="button">
                                    View Applications
                                </a>
                                <br><br><br><br>
                                <font class="row1_child_font1"
                                    style="border-radius:7px;padding-left:40px;padding-right:40px;padding-top:10px;padding-bottom:10px;background:orangered;color:white;">
                                    View Applications</font>
                            </center>
                        </div>
                    </span>

                </span>


            </span></span>
        <br>
        <span class="row" id="row3">
            <span class="col-md-12">
                <div class="row1_child" style="height:100%;" id="chart">




                </div>

            </span></span>
    </div>
    <script>
        var manageapplication = document.getElementById("manageapplication");
        for (i = 1; i <= 1000; i++) {
            var div = document.createElement("div");
            div.id = "manage" + i;
            div.style.border = "1px solif #f8f8f8";
            div.style.background = "white";
            if (i % 2 != 0) {
                div.style.background = "#f8f8f8";
            }
            var br1 = document.createElement("br");
            var img = document.createElement("img");
            img.src = "images/emailusr.png";
            img.style.width = "40px";
            img.style.borderRadius = "50%";
            img.style.marginLeft = "20px";
            manageapplication.appendChild(div);
            div.appendChild(br1);
            div.appendChild(img);
            var appname = document.createTextNode("Danny Scoff");
            var namefont = document.createElement("font");
            namefont.appendChild(appname);
            namefont.style.fontFamily = 'Poppins';
            namefont.style.fontStyle = "normal";
            namefont.style.fontWeight = "bold";
            namefont.style.fontSize = "min(3vw,1.7rem)";
            namefont.style.margin = "10%";
            namefont.style.color = "#012A60";
            div.appendChild(namefont);
            var br2 = document.createElement("br");
            div.appendChild(br2);
            var appdept = document.createTextNode("English Teacher");
            var deptfont = document.createElement("font");
            deptfont.appendChild(appdept);
            deptfont.style.fontFamily = 'Poppins';
            deptfont.style.fontStyle = "normal";

            deptfont.style.fontSize = "min(3vw,1.2rem)";
            deptfont.style.margin = "27%";
            deptfont.style.color = "#012A60";
            div.appendChild(deptfont);


            var text3 = document.createTextNode("Hey John, wellish laminable ineunt popshop catalyte prismatize campimetrical lentisk excluding portlet coccinellid impestation Bangash Lollardist perameloid procerebrum");
            var font3 = document.createElement("font")
            font3.style.whiteSpace = "nowrap";
            var span = document.createElement("div");
            span.style.marginLeft = "27%";
            span.style.width = "73%";
            span.style.overflow = "hidden";
            font3.appendChild(text3);
            span.appendChild(font3);
            div.appendChild(span);

        }






        var schedule = document.getElementById("schedule");
        for (i = 1; i <= 10; i++) {
            var div2 = document.createElement("div");
            div2.id = "schedule" + i;
            div2.style.border = "1px solif #f8f8f8";
            div2.style.background = "white";
            div2.style.boxShadow = "2px 2px 3px 3px lightgrey";
            if (i % 2 != 0) {
                div.style.background = "#f8f8f8";
            }
            var br3 = document.createElement("br");

            schedule.appendChild(div2);
            div2.appendChild(br3);
            var icon = document.createElement("span");
            icon.className = "glyphicon glyphicon-list";
            icon.style.marginLeft = "5%";
            div2.appendChild(icon);

            var schname = document.createTextNode("Business Communication");
            var schfont = document.createElement("font");
            schfont.appendChild(schname);
            schfont.style.fontFamily = 'Poppins';
            schfont.style.fontStyle = "normal";
            schfont.style.fontWeight = "bold";
            schfont.style.fontSize = "min(3vw,1.7rem)";
            schfont.style.margin = "5%";
            schfont.style.color = "#012A60";
            div2.appendChild(schfont);

            var course = document.createTextNode("View Course >");
            var coursefont = document.createElement("font");
            coursefont.appendChild(course);
            coursefont.style.fontFamily = 'Poppins';
            coursefont.style.fontStyle = "normal";
            coursefont.style.color = "#6853F8";
            coursefont.style.fontSize = "min(3vw,1.2rem)";
            coursefont.style.marginLeft = "27%";

            div2.appendChild(coursefont);

            var br4 = document.createElement("br");
            div2.appendChild(br4);
            var dttext3 = document.createTextNode("17 october 2021");
            var dtfont3 = document.createElement("font")
            dtfont3.style.color = "lightgrey";
            dtfont3.style.marginLeft = "12%";
            dtfont3.appendChild(dttext3);

            div2.appendChild(dtfont3);

        }


        //chart------------------------------------------------
        var options = {
            series: [{
                name: "State Employment",
                data: [1000, 12000, 3000, 9000, 5000, 10000, 11000, 7000, 6000, 12000, 4000, 8000, 10000]
            }],
            chart: {
                type: 'area',
                height: 350,
                zoom: {
                    enabled: false
                }
            },
            dataLabels: {
                enabled: false
            },
            stroke: {
                curve: 'straight'
            },

            title: {
                text: 'State Employment for last 12 years',
                align: 'left'
            },

            labels: [2010, 2011, 2012, 2013, 2014, 2015, 2016, 2017, 2018, 2019, 2020, 2021, 2022],
            xaxis: {

            },
            yaxis: {
                opposite: true
            },
            legend: {
                horizontalAlign: 'left'
            }
        };

        var chart = new ApexCharts(document.querySelector("#chart"), options);
        chart.render();

    </script>
</body>

</html>