                        <script>
                            var theme = document.getElementById("theme");
                            var maincontentcont = document.getElementById("maincontentcont");
                            var navbar = document.getElementById("navbar");
                            var dropbtn = document.getElementById("dropbtn");
                            var a = 0;
                            theme.addEventListener("click", () => {
                                if (a % 2 == 0) {
                                    dropbtn.style.background = "#161d30";
                                    for (i = 1; i <= 3; i++) {
                                        document.getElementById("cont" + i).style.background = "#303751";
                                        document.getElementById("cont" + i).style.color = "white";
                                        document.getElementById("cont" + i).style.boxShadow = "0px 5px 5px 1px #161d30";
                                    }
                                    document.getElementById("navbar").style.background = "#292f45";
                                    document.getElementById("navbar").style.color = "white";
                                    document.body.style.backgroundColor = "#161d30";
                                    maincontentcont.style.background = "#292f45";
                                    maincontentcont.style.color = "white";
                                    document.getElementById("bottom_cont").style.background = "#292f45";
                                    for (i = 1; i <= 3; i++) {
                                        document.getElementById("notification" + i).style.background = "#5c699b";
                                        document.getElementById("notification" + i).style.color = "white";
                                    }
                                    for (i = 1; i <= 12; i++) {

                                        document.getElementById("link" + i).style.color = "white";
                                    }
                                    for (x = 1; x <= 6; x++) {
                                        document.getElementById("navbt" + x).style.background = "#292f45";
                                        document.getElementById("navbt" + x).style.color = "white";
                                    }
                                }
                                else {
                                    dropbtn.style.background = "lightgrey";
                                    for (i = 1; i <= 3; i++) {
                                        document.getElementById("cont" + i).style.background = "white";
                                        document.getElementById("cont" + i).style.color = "#202020";
                                        document.getElementById("cont" + i).style.boxShadow = "0px 5px 5px 1px lightgrey";
                                    }
                                    document.getElementById("navbar").style.background = "white";
                                    document.getElementById("navbar").style.color = "#202020";
                                    document.body.style.backgroundColor = "#f2f2f2";
                                    maincontentcont.style.background = "white";
                                    maincontentcont.style.color = "#202020";
                                    document.getElementById("bottom_cont").style.background = "white";
                                    for (i = 1; i <= 3; i++) {
                                        document.getElementById("notification" + i).style.background = "#C8ECF4";
                                        document.getElementById("notification" + i).style.color = "#202020";
                                    }
                                    for (i = 1; i <= 12; i++) {

                                        document.getElementById("link" + i).style.color = "#202020";
                                    }
                                    for (x = 1; x <= 6; x++) {
                                        document.getElementById("navbt" + x).style.background = "white";
                                        document.getElementById("navbt" + x).style.color = "#202020";
                                    }





                                }
